{
  "name": "@no-witness-labs/hydra-sdk-monorepo",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "type-check": "turbo type-check",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,mjs,json,md}\"",
    "test": "turbo test",
    "coverage": "turbo test -- --coverage",
    "clean": "turbo clean && rimraf packages/*/dist packages/*/.turbo",
    "circular": "madge --circular --extensions ts packages/*/src/index.ts",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm build && changeset publish",
    "docgen": "turbo docgen"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@effect/docgen": "^0.5.2",
    "@effect/vitest": "^0.27.0",
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "eslint": "^10.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "madge": "^8.0.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "turbo": "^2.8.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "effect": "^3.19.15"
  },
  "dependencies": {
    "@effect/docgen": "^0.5.2",
    "@effect/platform": "^0.94.2"
  }
}
